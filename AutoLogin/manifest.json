{
	"manifest_version": 3,
	"name": "オートログイン",
	"version": "1.0.0",
	"description": "オートログインをします",
	"action": {
		"default_title": "AutoLogin",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs"
	],
	"content_scripts": [
		{	
			"matches": ["https://youtube.com/*"],
			"js": ["content.js"]
		}
	],
	"icons": {
		"128": "icon.png"
	}
}